[
    { "title": "关雎", "chapter": "周南", "content": ["关关雎鸠，在河之洲。", "窈窕淑女，君子好逑。", "参差荇菜，左右流之。", "窈窕淑女，寤寐求之。"] },
    { "title": "葛覃", "chapter": "周南", "content": ["葛之覃兮，施于中谷，维叶戚戚。", "黄鸟于飞，集于灌木，其鸣阶阶。"] },
    { "title": "桃夭", "chapter": "周南", "content": ["桃之夭夭，灼灼其华。", "之子于归，宜其室家。", "桃之夭夭，有蕡其实。", "之子于归，宜其家室。"] },
    { "title": "汉广", "chapter": "周南", "content": ["南有乔木，不可休思。", "汉有游女，不可求思。", "汉之广矣，不可泳思。", "江之永矣，不可方思。"] },
    { "title": "鹊巢", "chapter": "召南", "content": ["维鹊有巢，维鸠居之。", "之子于归，百两御之。"] },
    { "title": "采蘩", "chapter": "召南", "content": ["于以采蘩？于沼于沚。", "于以用之？公侯之事。"] },
    { "title": "草虫", "chapter": "召南", "content": ["喓喓草虫，趯趯阜螽。", "未见君子，忧心忡忡。", "亦既见止，亦既觏止，我心则降。"] },
    { "title": "采柏", "chapter": "召南", "content": ["泛彼柏舟，在彼中河。", "髧彼两髦，实维我仪。"] },
    { "title": "甘棠", "chapter": "召南", "content": ["蔽芾甘棠，勿剪勿伐，召伯所茇。", "蔽芾甘棠，勿剪勿败，召伯所憩。"] },
    { "title": "野有死麕", "chapter": "召南", "content": ["野有死麕，白茅包之。", "有女怀春，吉士诱之。"] },
    { "title": "燕燕", "chapter": "邶风", "content": ["燕燕于飞，差池其羽。", "之子于归，远送于野。", "瞻望弗及，泣涕如雨。"] },
    { "title": "凯风", "chapter": "邶风", "content": ["凯风自南，吹彼棘心。", "棘心夭夭，母氏劬劳。"] },
    { "title": "击鼓", "chapter": "邶风", "content": ["击鼓其镗，踊跃用兵。", "死生契阔，与子成说。", "执子之手，与子偕老。"] },
    { "title": "雄雉", "chapter": "邶风", "content": ["雄雉于飞，泄泄其羽。", "我之怀矣，自诒伊阻。"] },
    { "title": "谷风", "chapter": "邶风", "content": ["习习谷风，以阴以雨。", "黾勉同心，不宜有怒。"] },
    { "title": "式微", "chapter": "邶风", "content": ["式微式微，胡不归？", "微君之故，胡为乎中露？"] },
    { "title": "静女", "chapter": "邶风", "content": ["静女其姝，俟我于城隅。", "爱而不见，搔首沉吟。", "静女其娈，贻我彤管。", "彤管有炜，说怿女美。"] },
    { "title": "二子乘舟", "chapter": "邶风", "content": ["二子乘舟，泛泛其景。", "愿言思子，中心养养！"] },
    { "title": "柏舟", "chapter": "鄘风", "content": ["泛彼柏舟，在彼中河。", "髧彼两髦，实维我仪。"] },
    { "title": "定之方中", "chapter": "鄘风", "content": ["定之方中，作于楚宫。", "揆之以日，作于楚室。"] },
    { "title": "淇奥", "chapter": "卫风", "content": ["瞻彼淇奥，绿竹猗猗。", "有匪君子，如切如磋，如琢如磨。"] },
    { "title": "硕人", "chapter": "卫风", "content": ["硕人其颀，衣锦褧衣。", "手如柔荑，肤如凝脂，领如蝤蛴，齿如瓠犀，螓首蛾眉。", "巧笑倩兮，美目盼兮。"] },
    { "title": "木瓜", "chapter": "卫风", "content": ["投我以木瓜，报之以琼琚。", "匪报也，永以为好也。", "投我以木桃，报之以琼瑶。"] },
    { "title": "子衿", "chapter": "郑风", "content": ["青青子衿，悠悠我心。", "纵我不往，子宁不嗣音？", "青青子佩，悠悠我思。", "纵我不往，子宁不来？"] },
    { "title": "女曰鸡鸣", "chapter": "郑风", "content": ["女曰鸡鸣，士曰昧旦。", "子兴视夜，明星有烂。", "将翱将翔，弋凫与雁。"] },
    { "title": "有女同车", "chapter": "郑风", "content": ["有女同车，颜如舜华。", "将翱将翔，佩玉琼琚。", "彼美孟姜，洵美且都。"] },
    { "title": "山有扶苏", "chapter": "郑风", "content": ["山有扶苏，隰有荷华。", "不见子都，乃见狂且。"] },
    { "title": "风雨", "chapter": "郑风", "content": ["风雨凄凄，鸡鸣喈喈。", "既见君子，云胡不夷？", "风雨潇潇，鸡鸣胶胶。"] },
    { "title": "野有蔓草", "chapter": "郑风", "content": ["野有蔓草，零露漙兮。", "有美一人，清扬婉兮。", "邂逅相遇，适我愿兮。"] },
    { "title": "溱洧", "chapter": "郑风", "content": ["溱与洧，方涣涣兮。", "士与女，方秉蕳兮。", "维士与女，伊其相谑，赠之以勺药。"] },
    { "title": "鸡鸣", "chapter": "齐风", "content": ["鸡既鸣矣，朝既盈矣。", "匪鸡则鸣，苍蝇之声。", "东方明矣，朝既昌矣。"] },
    { "title": "卢令", "chapter": "齐风", "content": ["卢令令，其人美且仁。", "卢重环，其人美且鬈。", "卢重鋂，其人美且才。"] },
    { "title": "猗嗟", "chapter": "齐风", "content": ["猗嗟昌兮，颀而长兮。", "抑若扬兮，美目扬兮。", "巧趋跄兮，射则臧兮。"] },
    { "title": "伐檀", "chapter": "魏风", "content": ["坎坎伐檀兮，置之河之干兮，河水清且涟猗。", "不稼不穑，胡取禾三百缠兮？"] },
    { "title": "硕鼠", "chapter": "魏风", "content": ["硕鼠硕鼠，无食我黍！", "三岁贯女，莫我肯顾。", "逝将去女，适彼乐土。", "乐土乐土，爰得我所。"] },
    { "title": "蟋蟀", "chapter": "唐风", "content": ["蟋蟀在堂，岁聿其莫。", "今我不乐，日月其除。", "无已大康，职思其居。", "好乐无荒，良士休休。"] },
    { "title": "绸缪", "chapter": "唐风", "content": ["绸缪束薪，三星在天。", "今夕何夕，见此良人。", "子兮子兮，如此良人何！"] },
    { "title": "蒹葭", "chapter": "秦风", "content": ["蒹葭苍苍，白露为霜。", "所谓伊人，在水一方。", "溯洄从之，道阻且长。", "溯游从之，宛在水中央。"] },
    { "title": "晨风", "chapter": "秦风", "content": ["鴥彼晨风，郁彼北林。", "未见君子，忧心钦钦。", "如何如何，忘我实多！"] },
    { "title": "无衣", "chapter": "秦风", "content": ["岂曰无衣？与子同袍。", "王于兴师，修我戈矛。", "与子同仇！"] },
    { "title": "鹿鸣", "chapter": "小雅", "content": ["呦呦鹿鸣，食野之苹。", "我有嘉宾，鼓瑟吹笙。", "吹笙鼓簧，承筐是将。", "人之好我，示我周行。"] },
    { "title": "常棣", "chapter": "小雅", "content": ["常棣之华，鄂不韡韡。", "凡今之人，莫如兄弟。"] },
    { "title": "伐木", "chapter": "小雅", "content": ["伐木丁丁，鸟鸣嘤嘤。", "出自幽谷，迁于乔木。", "嘤其鸣矣，求其友声。"] },
    { "title": "天保", "chapter": "小雅", "content": ["天保定尔，亦孔之固。", "俾尔单厚，何福不除？", "如月之恒，如日之升。", "如南山之寿，不骞不崩。", "如松柏之茂，无不尔或承。"] },
    { "title": "采薇", "chapter": "小雅", "content": ["采薇采薇，薇亦作止。", "曰归曰归，岁亦莫止。", "昔我往矣，杨柳依依。", "今我来思，雨雪霏霏。"] },
    { "title": "南有嘉鱼", "chapter": "小雅", "content": ["南有嘉鱼，蒸然罩罩。", "君子有酒，嘉宾式燕以乐。"] },
    { "title": "南山有台", "chapter": "小雅", "content": ["南山有台，北山有莱。", "乐只君子，邦家之基。", "乐只君子，万寿无期。"] },
    { "title": "鹤鸣", "chapter": "小雅", "content": ["鹤鸣于九皋，声闻于野。", "鱼潜在渊，或在于渚。", "乐彼之园，爰有树檀，其下维萚。", "它山之石，可以攻玉。"] },
    { "title": "白驹", "chapter": "小雅", "content": ["皎皎白驹，食我场苗。", "絷之维之，以永今朝。", "所谓伊人，于焉嘉客。"] },
    { "title": "斯干", "chapter": "小雅", "content": ["秩秩斯干，幽幽南山。", "如竹苞矣，如松茂矣。", "兄及弟矣，式相好矣，无相犹矣。"] },
    { "title": "隰桑", "chapter": "小雅", "content": ["隰桑有阿，其叶有难。", "既见君子，其乐如何。", "心乎爱矣，遐不谓矣，中心藏之，何日忘之。"] },
    { "title": "维天之命", "chapter": "周颂", "content": ["维天之命，于穆不已。", "于乎不显，文王之德之纯。", "假以溢我，我其收之。"] }
  ]